;; ------- iterative -------
(let beforeLast 0)
(let lst 1)
(for (_ i) 0..15
    (let t (+ beforeLast lst))
    (put t)
    (let beforeLast lst)
    (let lst t)) 

(put "--------------")
;; ------- recursive -------
(fun fib (_ a b i)
    (let r (+ a b))
    (put r)
    (if (not (eq i 15))
        (fib b r (+ i 1))))
(fib 0 1 0)
