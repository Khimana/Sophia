(fun fizzBuzz (_ n)
    (let mod3 (eq 0 (% n 3)))
    (let mod5 (eq 0 (% n 5)))

    (if mod3 (put "Fizz"))

    (if mod5 (put "Buzz"))

    (if (and mod3 mod5) 
        (put "FizzBuzz"))

    (if (and (not mod3) (not mod5))
        (put n))

    (if (not (eq n 15)) 
        (fizzBuzz (+ n 1)))
)
(fizzBuzz 1)
