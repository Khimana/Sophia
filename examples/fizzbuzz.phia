(fun fizzBuzz (_ n)
    (let modThree (eq 0 (% n 3)))
    (let modFive (eq 0 (% n 5)))

    (if modThree (put "Fizz"))

    (if modFive (put "Buzz"))

    (if (and modThree modFive) 
        (put "FizzBuzz"))

    (if (and (not modThree) (not modFive))
        (put n))

    (if (not (eq n 15)) 
        (fizzBuzz (+ n 1)))
)
(fizzBuzz 1)
