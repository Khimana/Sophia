(fun fizzBuzz (_) 
    (let modThree (eq 0 (% i 3)))
    (let modFive (eq 0 (% i 5)))

    (if modThree
         (put "Fizz"))

    (if modFive
         (put "Buzz"))

    (if (and modThree modFive)
        (put "FizzBuzz"))

    (if 
        (and 
            (not modThree) 
            (not modFive))
        (put i))

    (if (not (eq i 15))
        (let i 
            (+ i 1))
        (fizzBuzz))
)

(let i 1)
(fizzBuzz)

